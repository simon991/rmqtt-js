{
  "name": "rmqtt-server",
  "version": "0.1.0",
  "description": "High-performance MQTT server for Node.js built with Rust and Neon",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "npm run clean && npm run build:native:release && npm run build:ts",
    "build:native:release": "cargo build --release --message-format=json-render-diagnostics && node scripts/copy-native.js",
    "build:native:install": "cargo build --release --message-format=json-render-diagnostics && node scripts/copy-native.js",
    "build:ts": "tsc",
    "install": "node scripts/install.js",
    "test": "npm run build:native:release && mocha --require ts-node/register \"test/**/*.test.ts\"",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build:ts",
    "pack:check": "npm pack --dry-run",
    "example": "npm run build && node examples/simple-server.ts --experimental-strip-types"
  },
  "license": "MIT",
  "dependencies": {
    "node-gyp-build": "^4.8.2"
  },
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/mqtt": "^0.0.34",
    "@types/node": "^20.19.16",
    "chai": "^6.0.1",
    "mocha": "^10.0.0",
    "mqtt": "^5.14.1",
    "typescript": "^5.9.2",
    "ts-node": "^10.9.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/splitbuddies/mqtt-pubsub-adapter.git"
  },
  "keywords": [
    "mqtt",
    "pubsub",
    "broker",
    "server",
    "rmqtt",
    "iot",
    "websocket",
    "tls",
    "typescript"
  ],
  "author": "Simon Buchholz",
  "bugs": {
    "url": "https://github.com/splitbuddies/mqtt-pubsub-adapter/issues"
  },
  "homepage": "https://github.com/splitbuddies/mqtt-pubsub-adapter#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist/",
    "prebuilds/",
    "src/",
    "Cargo.toml",
    "Cargo.lock",
    "README.md",
    "LICENSE"
  ]
}